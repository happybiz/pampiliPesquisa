extends ./_layouts/pesquisa.pug

block content
    article.col-md-offset-1.col-md-10.col-lg-offset-2.col-lg-8
        .pampili-logo
            a(href="/")
                img.img-responsive(src="/img/rosa-pampili.svg", alt="Pampili")
        .container
            .row
                .col-sm-12.componente-pesquisa.pd-20-bottom.printPage
                    .row.well.no-bottom.participante
                        h3#titulo Resultado
                        h6 Fulano de Tal
                        .academia-canvas
                            .row
                                .col-sm-12.text-center
                                    canvas#canvas2
                    
                        




                .col-sm-12.text-center.pd-40-bottom
                    button#gerarPdf.btn.btn-default Gerar PDF

        img(src="img/pesquisa/roda.svg", alt="", class="hidden", id="roda")
        img(src="img/pesquisa/rosalina.svg", alt="", class="hidden", id="rosalina")
        img(src="img/pesquisa/eixo.svg", alt="", class="hidden", id="eixo")
        img(src="img/pesquisa/logo.svg", alt="", class="hidden", id="logo")
        img(src="img/pesquisa/sombra.svg", alt="", class="hidden", id="sombra")

block append scriptsJS 
    script(src="js/app.js")
    script(src="js/resultado-roda.js")